{
  "name": "typed-router",
  "version": "0.2.0",
  "main": "index.js",
  "author": {
    "name": "Victor Kotseruba",
    "email": "barbuzaster@gmail.com",
    "url": "https://github.com/barbuza"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/barbuza/typed-router.git"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": "in-publish && npm-run-all clean lint test build || not-in-publish",
    "test": "jest --coverage",
    "lint": "tslint -p tsconfig.json",
    "start": "jest --watch",
    "clean": "rimraf index.js index.js.map",
    "build": "tsc"
  },
  "peerDependencies": {
    "@types/react": "^15.0.0",
    "react": "^15.0.0"
  },
  "dependencies": {
    "ts-url-pattern": "^0.1.1"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^19.2.3",
    "@types/react": "^15.0.25",
    "@types/react-dom": "^15.5.0",
    "coveralls": "^2.13.1",
    "enzyme": "^2.8.2",
    "in-publish": "^2.0.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "ts-jest": "^20.0.4",
    "ts-node": "^3.0.4",
    "tslint": "^5.3.2",
    "typescript": "^2.3.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "mapCoverage": true,
    "testRegex": "-test\\.tsx$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "index.tsx"
    ]
  }
}
